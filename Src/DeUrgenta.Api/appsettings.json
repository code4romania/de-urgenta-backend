    "IpWhitelist": [], // use these to bypass localhost and private addresses "127.0.0.1", "::1/10", "192.168.0.0/24"
    "EndpointWhitelist": [], // examples: "get:/api/license", "*:/api/status"
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*:/auth/register",
        "Period": "5s",
        "Limit": 2
      },
      {
        "Endpoint": "*:/auth/register",
        "Period": "1m",
        "Limit": 5
      }
    ]
